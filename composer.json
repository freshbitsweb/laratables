{
    "name": "freshbitsweb/laratables",
    "description": "Ajax support of DataTables for Laravel",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.33.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.9|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.3.9|^10.0",
        "laravel/legacy-factories": "^1.3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gaurav Makhecha",
            "email": "info@freshbits.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\Laratables\\LaratablesServiceProvider"
            ]
        }
    }
}
