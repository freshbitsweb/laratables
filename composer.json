{
    "name": "matthaselip/laratables",
    "description": "Ajax support of DataTables for Laravel 5.5+ to v10",
    "require": {
        "php": "^7.3|^7.4|^8.0|^8.1",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0.0|^8.0.0",
        "phpunit/phpunit": "^9.3.9|^10.0.0",
        "laravel/legacy-factories": "^1.3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matt",
            "email": "support@laratables.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\Laratables\\LaratablesServiceProvider"
            ]
        }
    }
}
